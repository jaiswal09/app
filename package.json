{
  "name": "medical-inventory-local",
  "version": "2.0.0",
  "description": "Local Medical Inventory Management System with Express.js backend and React frontend",
  "scripts": {
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && npm install && npm run db:generate && npm run db:push && npm run db:seed",
    "setup:frontend": "npm install",
    "dev": "npm run dev:backend & npm run dev:frontend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "vite",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "vite build",
    "start": "npm run start:backend & npm run start:frontend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "vite preview",
    "db:studio": "cd backend && npm run db:studio",
    "db:reset": "cd backend && npm run db:reset",
    "clean": "rm -rf backend/node_modules node_modules backend/dist dist",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "keywords": [
    "medical",
    "inventory",
    "management",
    "express",
    "react",
    "prisma",
    "local",
    "healthcare"
  ],
  "author": "Medical Inventory Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "dependencies": {
    "@tanstack/react-query": "^5.18.1",
    "axios": "^1.6.7",
    "date-fns": "^3.0.6",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "openai": "^4.28.0",
    "papaparse": "^5.4.1",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.21.3",
    "recharts": "^2.10.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.3.14",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.0.2"
  }
}
